{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
        "appearanceTools": true,
        "shadow": {
            "presets": [
                {
                    "name": "Default",
                    "slug": "default",
                    "shadow": "0 .125rem .25rem rgba(0,0,0,.075)"
                }
            ]
        },
        "layout": {
            "allowCustomContentAndWideSize": true,
            "allowEditing": true,
            "contentSize": "1400px",
            "wideSize": "1700px"
        },
        "color": {
            "palette": [
                {
                    "name": "Black",
                    "slug": "black",
                    "color": "#000000"
                },
                {
                    "name": "Mighty Blue",
                    "slug": "mighty-blue",
                    "color": "#014CB1"
                },
                {
                    "name": "Mighty Orange",
                    "slug": "mighty-orange",
                    "color": "#F56504"
                },
                {
                    "name": "White",
                    "slug": "white",
                    "color": "#FFFFFF"
                }
            ],
            "defaultPalette": false
        },
        "typography": {
            "fontWeight": true,
            "fontFamilies": [
                {
                    "fontFamily": "Roboto, sans-serif",
                    "name": "Roboto",
                    "slug": "roboto",
                    "fontFace": [
                        {
                            "fontFamily": "Roboto",
                            "fontStyle": "normal",
                            "fontWeight": "300",
                            "src": [
                                "file:./assets/src/fonts/Roboto/Roboto-Light.woff",
                                "file:./assets/src/fonts/Roboto/Roboto-Light.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Roboto",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:./assets/src/fonts/Roboto/Roboto-Regular.woff",
                                "file:./assets/src/fonts/Roboto/Roboto-Regular.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Roboto",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./assets/src/fonts/Roboto/Roboto-Bold.woff",
                                "file:./assets/src/fonts/Roboto/Roboto-Bold.woff2"
                            ]
                        }
                    ]
                }
            ],
            "fontSizes": [
                {
                    "name": "Small",
                    "slug": "small",
                    "size": "0.875rem"
                },
                {
                    "name": "Medium",
                    "slug": "medium",
                    "size": "1rem"
                },
                {
                    "name": "Large",
                    "slug": "large",
                    "size": "1.5rem"
                },
                {
                    "name": "X-Large",
                    "slug": "x-large",
                    "size": "2rem"
                }
            ]
        },
        "spacing": {
            "spacingSizes": [
                {
                    "name": "X-Small",
                    "slug": "x-small",
                    "size": "5px"
                },
                {
                    "name": "Small",
                    "slug": "small",
                    "size": "10px"
                },
                {
                    "name": "Medium",
                    "slug": "medium",
                    "size": "20px"
                },
                {
                    "name": "Large",
                    "slug": "large",
                    "size": "40px"
                },
                {
                    "name": "X-Large",
                    "slug": "x-large",
                    "size": "80px"
                }
            ]
        }
    },
	"styles": {
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--roboto)",
            "fontWeight": "400"
        }
    },
	"customTemplates": {},
	"templateParts": [
        {
            "area": "header",
            "name": "header-signed-in",
            "title": "Header - Signed-in"
        }
    ],
	"patterns": []
}